{
  "name": "portfolio",
  "private": true,
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "Chris Sheppard <chris@chrissheppard.co.uk> (http://www.chrissheppard.co.uk/)",
  "license": "GPLv3",
  "scripts": {
    "start": "cross-env NODE_ENV=production npm run build && npm run server:start",
    "start:dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run server:watch\" \"npm run webpack:watch\"",
    "build": "cross-env NODE_ENV=production npm run webpack:build",
    "build:dev": "cross-env NODE_ENV=development npm run webpack:build",
    "server:start": "babel-node src",
    "server:watch": "nodemon --watch src --exec babel-node src",
    "webpack:build": "webpack --config webpack/webpack.config.prod.js",
    "webpack:watch": "webpack-dev-server --config webpack/webpack.config.dev.js",
    "test": "jest",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg"
  },
  "dependencies": {
    "autoprefixer": "6.7.1",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
